{
    "search-engine-dropdown-root": {
        "selectorData": "defaultEngine",
        "strategy": "id",
        "groups": [
            "requiredForPage",
            "dropdownRoot",
            "searchEngineDropdown"
        ]
    },

    "search-engine-dropmarker": {
        "selectorData": "dropmarker",
        "strategy": "tag",
        "shadowParent": "search-engine-dropdown-root",
        "groups": [
            "searchEngineDropdown"
        ]
    },

    "dropdown-by-label": {
        "selectorData": ".//label[contains(., '{label}')]/following-sibling::hbox/menulist",
        "strategy": "xpath",
        "groups": [
            "generic",
            "dropdownRoot"
        ]
    },

    "dropdown-by-current-value": {
        "selectorData": "menulist[label='{value}']",
        "strategy": "css",
        "groups": [
            "generic",
            "dropdownRoot"
        ]
    },

    "active-menu-item": {
        "selectorData": "menuitem[_moz-menuactive='true']",
        "strategy": "css",
        "groups": [
            "refresh"
        ]
    },

    "find-in-settings": {
        "selectorData": "searchInput",
        "strategy": "id",
        "groups": [
            "requiredForPage"
        ]
    },

    "find-in-settings-input": {
        "selectorData": "input[placeholder='Find in Settings']",
        "strategy": "css",
        "shadowParent": "find-in-settings",
        "groups": []
    },

    "h2-enhanced-tracking-protection": {
        "selectorData": "h2[data-l10n-id='content-blocking-enhanced-tracking-protection']",
        "strategy": "css",
        "groups": []
    },

    "save-and-fill-addresses": {
        "selectorData": "checkbox[label='Save and fill addresses']",
        "strategy": "css",
        "groups": []
    },

    "save-and-fill-payment-methods": {
        "selectorData": "checkbox[label='Save and fill payment methods']",
        "strategy": "css",
        "groups": []
    },
    "saved-payment-methods-button": {
        "selectorData": "button[label='Saved payment methods']",
        "strategy": "css",
        "groups": []
    },

    "add_card_button": {
        "selectorData": "add",
        "strategy": "id",
        "groups": []
    },

    "cc_name_input": {
        "selectorData": "#cc-name",
        "strategy": "css",
        "groups": []
    },

    "cc_number_input": {
        "selectorData": "#cc-number",
        "strategy": "css",
        "groups": []
    },

    "cc_exp_month_input": {
        "selectorData": "#cc-exp-month",
        "strategy": "css",
        "groups": []
    },

    "cc_exp_year_input": {
        "selectorData": "#cc-exp-year",
        "strategy": "css",
        "groups": []
    },

    "cc_cvc_input": {
        "selectorData": "#cc-cvc",
        "strategy": "css",
        "groups": []
    },

    "save_card_button": {
        "selectorData": "button[data-l10n-id='save-credit-card-button']",
        "strategy": "css",
        "groups": []
    }
}

{
    "context": "chrome",
    "tab-bar-root": {
        "selectorData": "tabbrowser-arrowscrollbox",
        "strategy": "id",
        "groups": [
            "requiredForPage"
        ]
    },

    "tab-by-title": {
        "selectorData": "tab[label=\"{title}\"]",
        "strategy": "css",
        "groups": [
            "tabSelector"
        ]
    },

    "tab-by-index": {
        "selectorData": "tab[linkedpanel$=\"-{index}\"]",
        "strategy": "css",
        "groups": [
            "tabSelector"
        ]
    },

    "newtab-button": {
        "selectorData": "tabs-newtab-button",
        "strategy": "id",
        "groups": [
            "requiredForPage"
        ]
    },

    "tab-icon-overlay": {
        "selectorData": "tab-icon-overlay",
        "strategy": "class",
        "groups": []
    },

    "tab-sound-label": {
        "selectorData": "tab-icon-sound-{status}-label",
        "strategy": "class",
        "groups": []
    },

    "tab-title": {
        "selectorData": "tab-text",
        "strategy": "class",
        "groups": [
            "subTabOnly"
        ]
    },

    "list-all-tabs-button": {
        "selectorData": "alltabs-button",
        "strategy": "id",
        "groups": [
            "requiredForPage",
            "doNotCache"
        ]
    },

    "all-tabs-menu": {
        "selectorData": "allTabsMenu-allTabsView",
        "strategy": "id",
        "groups": [
            "allTabsMenu"
        ]
    },

    "all-tabs-entry": {
        "selectorData": "all-tabs-item",
        "strategy": "class",
        "groups": [
            "allTabsMenu"
        ]
    },

    "all-tabs-entry-selected": {
        "selectorData": "toolbaritem.all-tabs-item[selected='true']",
        "strategy": "css",
        "groups": [
            "allTabsMenu"
        ]
    },

    "tab-scrollbox": {
        "selectorData": "tabbrowser-arrowscrollbox",
        "strategy": "id",
        "groups": []
    },

    "tab-scrollbox-left-button": {
        "selectorData": "scrollbutton-up",
        "strategy": "id",
        "shadowParent": "tab-scrollbox",
        "groups": []
    },

    "tab-scrollbox-right-button": {
        "selectorData": "scrollbutton-down",
        "strategy": "id",
        "shadowParent": "tab-scrollbox",
        "groups": []
    },

    "tab-icon": {
        "selectorData": "tab-icon-image",
        "strategy": "class",
        "groups": []
    }

}
